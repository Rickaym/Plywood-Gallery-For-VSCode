{
  "name": "plywood-gallery",
  "displayName": "Plywood Gallery For VSCode",
  "description": "A utility Extension to visualize code snippets for Python Graphical Frameworks.",
  "version": "0.0.1",
  "publisher": "Rickaym",
  "repository": {
    "url": "https://github.com/Rickaym/Plywood-Gallery",
    "type": "git",
    "directory": "src"
  },
  "author": {
    "name": "Ricky",
    "url": "https://github.com/Rickaym"
  },
  "engines": {
    "vscode": "^1.66.0"
  },
  "categories": [
    "Visualization",
    "Other"
  ],
  "keywords": [
    "plywood",
    "gallery",
    "code snippets"
  ],
  "activationEvents": [
    "onCommand:plywood-gallery.ImportRemote",
    "onCommand:plywood-gallery.ImportLocal",
    "onCommand:plywood-gallery.Open",
    "onCommand:plywood-gallery.ClearCache",
    "onCommand:plywood-gallery.Reset"
  ],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "plywood-gallery.ImportRemote",
        "title": "Plywood Gallery: Import a Gallery from GitHub."
      },
      {
        "command": "plywood-gallery.ImportLocal",
        "title": "Plywood Gallery: Import a Gallery from local disks."
      },
      {
        "command": "plywood-gallery.Open",
        "title": "Plywood Gallery: Open the gallery webview."
      },
      {
        "command": "plywood-gallery.ClearCache",
        "title": "Plywood Gallery: Forcibly clear cache, this usually automatic."
      },
      {
        "command": "plywood-gallery.Reset",
        "title": "Plywood Gallery: Forcibly remove all downloaded galleries."
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "webpack --watch",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.66.0",
    "@types/glob": "^7.2.0",
    "@types/mocha": "^9.1.0",
    "@types/node": "14.x",
    "@typescript-eslint/eslint-plugin": "^5.16.0",
    "@typescript-eslint/parser": "^5.16.0",
    "eslint": "^8.11.0",
    "glob": "^7.2.0",
    "mocha": "^9.2.2",
    "typescript": "^4.5.5",
    "ts-loader": "^9.2.8",
    "webpack": "^5.70.0",
    "webpack-cli": "^4.9.2",
    "@vscode/test-electron": "^2.1.3"
  },
  "dependencies": {
    "axios": "^0.26.1",
    "yaml": "^2.0.0"
  }
}
